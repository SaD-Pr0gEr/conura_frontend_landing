  align-items: flex-start;
  gap: 16px;
  position: relative;
}

@media (min-width: 1024px) {
  .how-we-work-step {
    gap: 20px;
    cursor: pointer;
  }
}

/* Step Point */
.how-we-work-step-point {
  position: absolute;
  left: -28px;
  top: 8px;
  width: 6px;
  height: 6px;
  background-color: #D9D9D9;
  border-radius: 50%;
  transition: background-color 0.5s cubic-bezier(0.4, 0, 0.2, 1), transform 0.3s ease;
  z-index: 1;
}

@media (min-width: 1024px) {
  .how-we-work-step-point {
    left: -38px;
  }
}

.how-we-work-step.active .how-we-work-step-point {
  background-color: var(--color-black-100);
  transform: scale(1.3);
}

/* Step Content */
