  color: var(--color-black-100);
  font-weight: var(--font-weight-medium);
}

/* Form footer for service modal */
.service-modal-form .form-footer {
  margin-top: 8px;
}

.form-submit-btn {
  width: 100%;
}

/* ========================================
   HOW WE WORK SECTION (Главная страница)
   ======================================== */

.how-we-work-section {
  padding: 40px 16px;
}

@media (min-width: 1024px) {
  .how-we-work-section {
    padding: 60px 140px;
  }
}

.how-we-work-title {
  font-family: var(--font-primary);
  font-size: 34px;
  font-weight: var(--font-weight-semibold);
  color: var(--color-black-100);
