.how-we-work-progress {
  position: absolute;
  left: 2px;
  top: 10px;
  bottom: 10px;
  width: 2px;
}

.how-we-work-progress-line {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #E9E9E9;
  border-radius: 1px;
}

.how-we-work-progress-fill {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 0%;
  background-color: var(--color-black-100);
  border-radius: 1px;
  transition: height 0.5s cubic-bezier(0.4, 0, 0.2, 1);
}

/* Step */
.how-we-work-step {
