  height: 190px;
  width: 100%;
}

.target-platforms-chart-ring {
  position: relative;
  width: 250px;
  height: 250px;
  margin: 0 auto;
}

.target-platforms-chart-svg {
  width: 100%;
  height: 100%;
  transform: translateY(-30px);
}

.target-platforms-chart-center {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -70%);
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
}

.target-platforms-chart-icon {
  width: 30px;
  height: 30px;
  margin-bottom: 8px;
}

.target-platforms-chart-icon svg {
  width: 100%;
  height: 100%;
}

.target-platforms-chart-percent {
  font-family: var(--font-primary);
  font-size: 22px;
  font-weight: var(--font-weight-semibold);
  color: #101010;
  line-height: 1;
}

.target-platforms-chart-label {
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-medium);
  color: #787878;
  line-height: 1;
}

.target-platforms-meta-badge {
  position: absolute;
  top: 0;
  right: 0;
  width: 28px;
  height: 18px;
}

.target-platforms-meta-badge svg {
  width: 100%;
  height: 100%;
}

.target-platforms-content {
  display: flex;
  flex-direction: column;
  flex: 1;
  justify-content: space-between;
}

.target-platforms-heading {
  padding-top: 20px;
  border-top: 1px solid #EEF0F2;
  display: flex;
  flex-direction: column;
  gap: 4px;
  margin-bottom: 12px;
}

.target-platforms-card-title {
  font-family: var(--font-primary);
  font-size: 22px;
  font-weight: var(--font-weight-semibold);
  color: #101010;
  margin: 0;
  line-height: 1.2;
}

.target-platforms-card-users {
  font-family: var(--font-primary);
  font-size: 15px;
  font-weight: var(--font-weight-regular);
  color: #3F3F3F;
  margin: 0;
}

.target-platforms-card-desc {
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-regular);
  color: #787878;
  line-height: 1.4;
  margin: 0 0 20px 0;
  flex: 1;
}

.target-platforms-card-source {
  font-family: var(--font-primary);
  font-size: 13px;
  font-weight: var(--font-weight-regular);
  color: #787878;
  margin: 0;
}

@media (min-width: 1024px) {
  .target-platforms-section {
    padding: 100px 140px;
  }

  .target-platforms-title {
    font-size: 55px;
    line-height: 60px;
    margin-bottom: 40px;
  }

  .target-platforms-cards {
    display: flex;
    gap: 20px;
    overflow-x: visible;
    scroll-snap-type: none;
  }

  .target-platforms-card {
    flex: 1;
    width: auto;
    min-width: 0;
    max-width: 427px;
    border-radius: 40px;
    padding: 30px 25px;
    gap: 30px;
  }

  .target-platforms-chart {
    height: 210px;
  }

  .target-platforms-chart-ring {
    width: 314px;
    height: 314px;
  }

  .target-platforms-chart-svg {
    transform: translateY(-50px);
  }

  .target-platforms-chart-center {
    transform: translate(-50%, -80%);
  }

  .target-platforms-chart-percent {
    font-size: 28px;
  }

  .target-platforms-chart-label {
    font-size: 18px;
  }

  .target-platforms-meta-badge {
    width: 34px;
    height: 22px;
  }

  .target-platforms-heading {
    padding-top: 30px;
    margin-bottom: 20px;
  }

  .target-platforms-card-title {
    font-size: 28px;
  }

  .target-platforms-card-users {
    font-size: 18px;
    font-weight: var(--font-weight-medium);
  }

  .target-platforms-card-desc {
    font-size: 16px;
    line-height: 21px;
    margin-bottom: 0;
  }

  .target-platforms-card-source {
    font-size: 16px;
    line-height: 21px;
  }

  .target-platforms-content {
    min-height: 266px;
  }
}

