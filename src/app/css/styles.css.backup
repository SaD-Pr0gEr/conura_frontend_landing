/**
 * Main Styles — Conura
 * Основные стили сайта
 */

/* ========================================
   RESET & BASE
   ======================================== */

*,
*::before,
*::after {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

html {
  font-size: 16px;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

body {
  font-family: var(--font-primary);
  font-size: var(--mobile-body-14-size);
  font-weight: var(--font-weight-regular);
  line-height: var(--mobile-body-14-line);
  color: var(--color-black-100);
  background-color: var(--color-light-bg);
  min-height: 100vh;
}

@media (min-width: 1024px) {
  body {
    font-size: var(--desktop-body-16-size);
    line-height: var(--desktop-body-16-line);
    font-weight: var(--font-weight-medium);
  }
}

a {
  color: inherit;
  text-decoration: none;
}

img {
  max-width: 100%;
  height: auto;
  display: block;
}

button {
  font-family: inherit;
  cursor: pointer;
  border: none;
  background: none;
}

ul, ol {
  list-style: none;
}

/* ========================================
   LAYOUT
   ======================================== */

.container {
  width: 100%;
  max-width: var(--container-mobile-width);
  margin: 0 auto;
  padding: 0 var(--container-mobile-padding);
}

@media (min-width: 1024px) {
  .container {
    max-width: calc(var(--container-desktop-width) + var(--container-desktop-padding) * 2);
    padding: 0 var(--container-desktop-padding);
  }
}

/* Full width container */
.container-fluid {
  width: 100%;
  padding: 0 var(--container-mobile-padding);
}

@media (min-width: 1024px) {
  .container-fluid {
    padding: 0 var(--container-desktop-padding);
  }
}

/* ========================================
   HEADER
   ======================================== */

.header {
  background-color: var(--color-light-bg);
  padding: 4px 16px;
  position: sticky;
  top: 0;
  z-index: 100;
}

@media (min-width: 1024px) {
  .header {
    padding: 8px 140px;
  }
}

.header-inner {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

/* Logo */
.header-logo {
  display: block;
  width: 100px;
  height: 65px;
  flex-shrink: 0;
  overflow: hidden;
  position: relative;
}

.header-logo img {
  position: absolute;
  width: 144%;
  height: 224%;
  top: -77%;
  left: -22%;
  max-width: none;
}

@media (min-width: 1024px) {
  .header-logo {
    width: 100px;
    height: 64px;
  }
}

/* Navigation */
.header-nav {
  display: none;
}

@media (min-width: 1024px) {
  .header-nav {
    display: flex;
    gap: 35px;
    align-items: center;
  }
}

.header-nav-item {
  font-family: var(--font-primary);
  font-size: 16px;
  font-weight: var(--font-weight-medium);
  line-height: normal;
  color: var(--color-black-80);
  padding: 5px 8px;
  border-radius: 5px;
  background-color: var(--color-light-bg);
  text-decoration: none;
  display: flex;
  align-items: center;
  gap: 0;
  transition: var(--transition-fast);
  border: none;
  cursor: pointer;
}

.header-nav-item:hover {
  color: var(--color-black-100);
}

.header-nav-item.active {
  background-color: var(--color-light-gray);
  padding: 10px 15px;
  border-radius: 100px;
  color: var(--color-black-100);
}

.header-nav-item.has-dropdown {
  gap: 4px;
}

.header-nav-arrow {
  width: 10px;
  height: 5px;
  flex-shrink: 0;
  transition: transform var(--transition-fast);
}

.header-nav-dropdown.open .header-nav-arrow {
  transform: rotate(180deg);
}

.header-nav-dropdown {
  position: relative;
}

.header-nav-dropdown-menu {
  display: none;
  position: absolute;
  top: 100%;
  left: 0;
  margin-top: 10px;
  background-color: var(--color-white);
  border-radius: 16px;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.15);
  padding: 12px 0;
  min-width: 220px;
  z-index: 100;
}

.header-nav-dropdown.open .header-nav-dropdown-menu {
  display: block;
}

.header-nav-dropdown-item {
  display: block;
  padding: 12px 20px;
  font-family: var(--font-primary);
  font-size: 16px;
  font-weight: var(--font-weight-medium);
  color: var(--color-black-80);
  text-decoration: none;
  transition: background-color var(--transition-fast), color var(--transition-fast);
}

.header-nav-dropdown-item:hover {
  background-color: var(--color-light-bg);
  color: var(--color-black-100);
}

/* Actions */
.header-actions {
  display: flex;
  align-items: center;
  gap: 8px;
}

@media (min-width: 1024px) {
  .header-actions {
    gap: 10px;
  }
}

/* Language */
.header-language {
  position: relative;
}

.header-language-btn {
  width: 32px;
  height: 32px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: transparent;
  border: none;
  cursor: pointer;
  padding: 0;
  border-radius: var(--radius-md);
}

@media (min-width: 1024px) {
  .header-language-btn {
    width: 42px;
    height: 42px;
  }
  
  .header-language-btn svg {
    width: 28px;
    height: 28px;
  }
}

.header-language-dropdown {
  position: absolute;
  top: calc(100% + 8px);
  right: 0;
  background-color: var(--color-white);
  border-radius: var(--radius-md);
  box-shadow: var(--shadow-dropdown);
  overflow: hidden;
  display: none;
  min-width: 60px;
  z-index: 101;
}

.header-language.open .header-language-dropdown {
  display: block;
}

.header-language-option {
  display: block;
  width: 100%;
  padding: 10px 15px;
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-medium);
  color: var(--color-black-100);
  background: transparent;
  border: none;
  cursor: pointer;
  text-align: left;
  transition: var(--transition-fast);
}

.header-language-option:hover {
  background-color: var(--color-light-gray);
}

.header-language-option.active {
  background-color: var(--color-light-gray);
}

/* CTA Button */
.header-cta {
  display: none;
}

@media (min-width: 1024px) {
  .header-cta {
    display: flex;
  }
}

/* Burger */
.header-burger {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 32px;
  height: 32px;
  background: transparent;
  border: none;
  cursor: pointer;
  padding: 0;
}

@media (min-width: 1024px) {
  .header-burger {
    display: none;
  }
}

/* ========================================
   MOBILE MENU
   ======================================== */

.mobile-menu {
  position: fixed;
  top: 0;
  right: 0;
  width: 100%;
  height: 100vh;
  background-color: var(--color-white);
  z-index: 1000;
  padding: 4px 16px 40px;
  display: flex;
  flex-direction: column;
  transform: translateX(100%);
  transition: transform 0.3s ease;
}

.mobile-menu.open {
  transform: translateX(0);
}

.mobile-menu-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 40px;
}

.mobile-menu-close {
  width: 32px;
  height: 32px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: transparent;
  border: none;
  cursor: pointer;
  padding: 0;
}

.mobile-menu-nav {
  display: flex;
  flex-direction: column;
  gap: 24px;
  flex: 1;
}

.mobile-menu-item {
  font-family: var(--font-primary);
  font-size: 28px;
  font-weight: var(--font-weight-semibold);
  line-height: normal;
  color: var(--color-black-100);
  text-decoration: none;
}

.mobile-menu-item:hover {
  color: var(--color-accent-sea);
}

.mobile-menu-footer {
  padding-top: 40px;
}

.mobile-menu-cta {
  width: 100%;
  justify-content: center;
}

/* ========================================
   MAIN
   ======================================== */

main {
  min-height: calc(100vh - 200px);
}

/* ========================================
   SECTIONS
   ======================================== */

.section {
  padding: 40px 0;
}

@media (min-width: 1024px) {
  .section {
    padding: 80px 0;
  }
}

.section-title {
  margin-bottom: 24px;
}

@media (min-width: 1024px) {
  .section-title {
    margin-bottom: 40px;
  }
}

/* ========================================
   BANNER SECTION
   ======================================== */

.banner-section {
  padding: 0 16px 40px;
  display: flex;
  flex-direction: column;
  gap: 20px;
}

@media (min-width: 1024px) {
  .banner-section {
    padding: 0 140px 60px;
    gap: 25px;
  }
}

/* Slider */
.banner-slider {
  position: relative;
  width: 100%;
  height: 520px;
  border-radius: 28px;
  overflow: hidden;
}

@media (min-width: 1024px) {
  .banner-slider {
    height: 550px;
    border-radius: 40px;
  }
}

.banner-slides {
  position: relative;
  width: 100%;
  height: 100%;
}

.banner-slide {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  visibility: hidden;
  transition: opacity 0.5s ease, visibility 0.5s ease;
}

.banner-slide.active {
  opacity: 1;
  visibility: visible;
}

.banner-slide-image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center top;
}

@media (min-width: 1024px) {
  .banner-slide-image {
    object-position: 50% 50%;
  }
}

/* Banner Content */
.banner-content {
  position: absolute;
  bottom: 25px;
  left: 16px;
  width: calc(100% - 32px);
  max-width: 296px;
  display: flex;
  flex-direction: column;
  gap: 15px;
}

@media (min-width: 1024px) {
  .banner-content {
    bottom: 60px;
    left: 50px;
    width: 600px;
    max-width: 600px;
    gap: 30px;
  }
}

.banner-title {
  font-family: var(--font-primary);
  font-size: 28px;
  font-weight: var(--font-weight-bold);
  line-height: normal;
  color: var(--color-black-100);
}

@media (min-width: 1024px) {
  .banner-title {
    font-size: 50px;
  }
}

/* Dark slide variant */
.banner-slide-dark .banner-title {
  color: var(--color-white);
}

/* Banner Subtitle */
.banner-subtitle {
  font-family: var(--font-primary);
  font-size: 16px;
  font-weight: var(--font-weight-semibold);
  line-height: 1.4;
  color: var(--color-black-100);
  margin: 10px 0 20px 0;
  max-width: 500px;
}

@media (min-width: 1024px) {
  .banner-subtitle {
    font-size: 28px;
    margin: 10px 0 30px 0;
    max-width: 600px;
  }
}

.banner-slide-dark .banner-subtitle {
  color: var(--color-white);
}

/* Service page banner slide content */
.banner-slide-content {
  position: absolute;
  bottom: 25px;
  left: 16px;
  width: calc(100% - 32px);
  max-width: 296px;
  display: flex;
  flex-direction: column;
  gap: 6px;
  z-index: 2;
}

@media (min-width: 1024px) {
  .banner-slide-content {
    bottom: 60px;
    left: 50px;
    width: 600px;
    max-width: 600px;
    gap: 10px;
  }
}

.banner-slide-title {
  font-family: var(--font-primary);
  font-size: 28px;
  font-weight: var(--font-weight-bold);
  line-height: 1;
  color: var(--color-white);
  margin: 0;
}

@media (min-width: 1024px) {
  .banner-slide-title {
    font-size: 50px;
  }
}

.banner-slide-subtitle {
  font-family: var(--font-primary);
  font-size: 18px;
  font-weight: var(--font-weight-semibold);
  line-height: 1.2;
  color: var(--color-white);
  margin: 0 0 10px 0;
}

@media (min-width: 1024px) {
  .banner-slide-subtitle {
    font-size: 28px;
    margin: 0 0 20px 0;
  }
}

.banner-cta {
  width: 100%;
  justify-content: center;
  padding: 16px 32px;
}

@media (min-width: 1024px) {
  .banner-cta {
    width: fit-content;
    padding: 16px 32px;
  }
}

/* Navigation */
.banner-navigation {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 50px;
}

@media (min-width: 1024px) {
  .banner-navigation {
    height: 60px;
  }
}

/* Pagination */
.banner-pagination {
  display: flex;
  gap: 8px;
  width: 120px;
}

@media (min-width: 1024px) {
  .banner-pagination {
    gap: 10px;
    width: auto;
  }
}

.banner-pagination-item {
  flex: 1;
  height: 2px;
  background-color: var(--color-black-100);
  opacity: 0.08;
  position: relative;
  overflow: hidden;
}

@media (min-width: 1024px) {
  .banner-pagination-item {
    flex: none;
    width: 50px;
  }
}

.banner-pagination-item.active {
  opacity: 1;
}

.banner-pagination-progress {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 0%;
  background-color: var(--color-black-100);
}

.banner-pagination-item.active .banner-pagination-progress {
  animation: banner-progress 4s linear forwards;
}

@keyframes banner-progress {
  from {
    width: 0%;
  }
  to {
    width: 100%;
  }
}

/* Arrows */
.banner-arrows {
  display: flex;
  gap: 5px;
}

@media (min-width: 1024px) {
  .banner-arrows {
    gap: 10px;
  }
}

.banner-arrow {
  width: 50px;
  height: 50px;
  border-radius: 30px;
  background-color: var(--color-light-gray);
  display: flex;
  align-items: center;
  justify-content: center;
  border: none;
  cursor: pointer;
  padding: 0;
  transition: background-color var(--transition-fast);
}

@media (min-width: 1024px) {
  .banner-arrow {
    width: 60px;
    height: 60px;
  }
}

.banner-arrow:hover {
  background-color: var(--color-light-stroke);
}

.banner-arrow svg {
  width: 10px;
  height: 16px;
}

@media (min-width: 1024px) {
  .banner-arrow svg {
    width: 12px;
    height: 20px;
  }
}

/* ========================================
   CERTIFICATE SECTION
   ======================================== */

.certificate-section {
  position: relative;
  display: flex;
  flex-direction: column;
  padding: 20px var(--container-mobile-padding) 0;
}

@media (min-width: 1024px) {
  .certificate-section {
    position: relative;
    padding: 0;
    min-height: 900px;
    overflow: hidden;
    margin-top: 0;
  }
}

/* Desktop Header: Title + IT Park Logo */
.certificate-header {
  display: none;
}

@media (min-width: 1024px) {
  .certificate-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    position: absolute;
    top: 40px;
    left: 144px;
    width: 1320px;
    z-index: 10;
  }
}

.certificate-heading {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  width: 670px;
  font-family: var(--font-primary);
  font-weight: var(--font-weight-semibold);
  font-size: 60px;
  line-height: 65px;
}

.certificate-heading-accent {
  color: var(--color-accent-sea);
  text-align: right;
  width: 100%;
}

.certificate-heading-text {
  color: var(--color-black-100);
  text-align: left;
  width: 100%;
}

.certificate-logo-desktop {
  width: 110px;
  height: 97px;
  object-fit: contain;
}

/* Scene Image */
.certificate-scene {
  width: 100%;
  position: relative;
  overflow: hidden;
  border-radius: 28px;
}

@media (min-width: 1024px) {
  .certificate-scene {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    border-radius: 0;
    z-index: 1;
  }
}

.certificate-scene-img {
  width: 100%;
  height: auto;
  display: block;
  transition: transform 0.3s ease-out;
}

@media (min-width: 1024px) {
  .certificate-scene-img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center 55%;
  }
}

/* Parallax effect */
.certificate-scene.parallax .certificate-scene-img {
  transform: translateY(var(--parallax-y, 0));
}

/* Mobile Content */
.certificate-content-mobile {
  display: flex;
  flex-direction: column;
  gap: 30px;
  margin-top: 20px;
}

@media (min-width: 1024px) {
  .certificate-content-mobile {
    display: none;
  }
}

.certificate-text {
  display: flex;
  flex-direction: column;
  gap: 15px;
}

.certificate-title {
  font-family: var(--font-primary);
  font-size: 34px;
  font-weight: var(--font-weight-semibold);
  line-height: 37px;
  color: var(--color-black-100);
  margin: 0;
}

.text-accent {
  color: var(--color-accent-sea);
}

.certificate-description {
  font-family: var(--font-primary);
  font-size: 15px;
  font-weight: var(--font-weight-regular);
  line-height: normal;
  color: var(--color-black-80);
  margin: 0;
}

/* Mobile Button */
.certificate-btn-mobile {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  width: 100%;
  height: 50px;
  padding: 16px 32px;
}

.certificate-btn-icon {
  width: 23px;
  height: 20px;
  object-fit: contain;
}

.certificate-btn-mobile:disabled {
  opacity: 0.6;
  cursor: not-allowed;
}

/* Desktop Round Button */
.certificate-btn-round {
  display: none;
}

@media (min-width: 1024px) {
  .certificate-btn-round {
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    left: 313px;
    bottom: 200px; /* Position from bottom */
    width: 140px;
    height: 140px;
    border-radius: 100px;
    background-color: var(--color-black-100);
    color: var(--color-white);
    font-family: var(--font-primary);
    font-size: 15px;
    font-weight: var(--font-weight-bold);
    line-height: 1.2;
    text-align: center;
    border: none;
    cursor: pointer;
    z-index: 10;
    transition: transform 0.2s ease, background-color 0.2s ease;
  }
  
  .certificate-btn-round:hover {
    transform: scale(1.05);
  }
  
  .certificate-btn-round:disabled {
    opacity: 0.6;
    cursor: not-allowed;
  }
  
  .certificate-btn-round:disabled:hover {
    transform: none;
  }
}

/* ========================================
   SERVICES SECTION (Accordion)
   ======================================== */

.services-section {
  padding: 40px var(--container-mobile-padding);
}

@media (min-width: 1024px) {
  .services-section {
    padding: 60px var(--container-desktop-padding);
  }
}

.services-title {
  font-family: var(--font-primary);
  font-size: 34px;
  font-weight: var(--font-weight-semibold);
  line-height: 37px;
  color: var(--color-black-100);
  margin: 0 0 30px;
}

@media (min-width: 1024px) {
  .services-title {
    font-size: 55px;
    line-height: 60px;
    margin: 0 0 35px;
  }
}

/* Services Content Container */
.services-content {
  display: flex;
  flex-direction: column;
}

@media (min-width: 1024px) {
  .services-content {
    flex-direction: row;
    gap: 40px;
  }
}

/* Services Tabs (Mobile: Accordion, Desktop: Tab list) */
.services-tabs {
  display: flex;
  flex-direction: column;
  width: 100%;
}

@media (min-width: 1024px) {
  .services-tabs {
    width: 418px;
    flex-shrink: 0;
  }
}

/* Individual Service Tab */
.service-tab {
  border-top: 1px solid var(--color-light-stroke);
  border-bottom: 1px solid var(--color-light-stroke);
  margin-top: -1px; /* Collapse borders */
  transition: border-color 0.3s ease;
}

.service-tab:first-child {
  margin-top: 0;
}

/* Tab Header */
.service-tab-header {
  display: flex;
  align-items: center;
  gap: 15px;
  padding: 25px 0;
  cursor: pointer;
}

@media (min-width: 1024px) {
  .service-tab-header {
    gap: 20px;
    padding: 24px 0;
  }
}

/* Tab Icon */
.service-tab-icon {
  width: 40px;
  height: 40px;
  border-radius: 10px;
  background-color: var(--color-light-gray);
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
  flex-shrink: 0;
  transition: background-color var(--transition-fast);
}

.service-tab-icon img {
  width: 24px;
  height: 24px;
  object-fit: contain;
  opacity: 0.3;
  filter: invert(1) brightness(0); /* Make white icon to black for visibility on gray bg */
  transition: opacity var(--transition-fast), filter var(--transition-fast);
}

.service-tab.active .service-tab-icon {
  background-color: var(--color-black-100);
}

.service-tab.active .service-tab-icon img {
  opacity: 1;
  filter: none; /* Reset filter - show white icon on black background */
}

/* Tab Name */
.service-tab-name {
  font-family: var(--font-primary);
  font-size: 20px;
  font-weight: var(--font-weight-medium);
  line-height: normal;
  color: var(--color-black-100);
  flex: 1;
  transition: color var(--transition-fast);
}

@media (min-width: 1024px) {
  .service-tab-name {
    font-size: 24px;
    color: var(--color-black-60);
  }
}

.service-tab.active .service-tab-name {
  font-weight: var(--font-weight-semibold);
  color: var(--color-black-100);
}

/* Tab Toggle Button (Mobile only) */
.service-tab-toggle {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30px;
  height: 30px;
  background: none;
  border: none;
  cursor: pointer;
  color: var(--color-black-100);
  padding: 0;
}

@media (min-width: 1024px) {
  .service-tab-toggle {
    display: none;
  }
}

.service-tab-toggle .icon-close {
  display: none;
}

.service-tab-toggle .icon-plus {
  display: block;
  transition: transform 0.3s ease;
}

.service-tab.active .service-tab-toggle .icon-close {
  display: block;
}

.service-tab.active .service-tab-toggle .icon-plus {
  display: none;
}

/* Rotate plus icon on hover */
.service-tab:not(.active) .service-tab-toggle:hover .icon-plus {
  transform: rotate(90deg);
}

/* Tab Content (Mobile: Expandable, Desktop: Hidden) */
.service-tab-content {
  display: flex;
  flex-direction: column;
  gap: 15px;
  padding-bottom: 0;
  max-height: 0;
  opacity: 0;
  overflow: hidden;
  transition: max-height 0.4s ease, opacity 0.3s ease, padding-bottom 0.3s ease;
}

.service-tab.active .service-tab-content {
  max-height: 800px; /* Large enough to fit content */
  opacity: 1;
  padding-bottom: 25px;
}

@media (min-width: 1024px) {
  .service-tab-content {
    display: none !important; /* Always hidden on desktop */
  }
}

/* Tab Image (Mobile) */
.service-tab-image {
  width: 100%;
  aspect-ratio: 328 / 357;
  border-radius: 20px;
  overflow: hidden;
  background-color: var(--color-light-gray);
}

.service-tab-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

/* Tab Description (Mobile) */
.service-tab-description {
  font-family: var(--font-primary);
  font-size: 15px;
  font-weight: var(--font-weight-regular);
  line-height: normal;
  color: var(--color-black-80);
  margin: 0;
}

/* Tab Button (Mobile) */
.service-tab-btn {
  width: 100%;
  padding: 16px 32px;
}

/* Desktop Preview (Hidden on Mobile) */
.services-preview {
  display: none;
}

@media (min-width: 1024px) {
  .services-preview {
    display: flex;
    gap: 40px;
    flex: 1;
  }
}

/* Preview Image */
.services-preview-image {
  width: 404px;
  height: 440px;
  border-radius: 20px;
  overflow: hidden;
  flex-shrink: 0;
}

.services-preview-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: opacity 0.3s ease;
}

/* Preview Content */
.services-preview-content {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 418px;
  height: 440px;
}

/* Preview Text */
.services-preview-text {
  display: flex;
  flex-direction: column;
  gap: 10px;
}

.services-preview-title {
  font-family: var(--font-primary);
  font-size: 32px;
  font-weight: var(--font-weight-semibold);
  line-height: normal;
  color: var(--color-black-100);
  margin: 0;
  transition: opacity 0.3s ease;
}

.services-preview-description {
  font-family: var(--font-primary);
  font-size: 18px;
  font-weight: var(--font-weight-medium);
  line-height: normal;
  color: var(--color-black-60);
  margin: 0;
  transition: opacity 0.3s ease;
}

/* Preview Button (Round) */
.services-preview-btn {
  width: 140px;
  height: 140px;
  border-radius: 100px;
  background-color: var(--color-black-100);
  color: var(--color-white);
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  font-family: var(--font-primary);
  font-size: 15px;
  font-weight: var(--font-weight-bold);
  line-height: 1.2;
  text-decoration: none;
  transition: transform var(--transition-fast), background-color var(--transition-fast);
}

.services-preview-btn:hover {
  transform: scale(1.05);
}

/* ========================================
   UTILITIES
   ======================================== */

/* Display */
.d-none { display: none; }
.d-block { display: block; }
.d-flex { display: flex; }
.d-grid { display: grid; }

/* Flex */
.flex-column { flex-direction: column; }
.flex-wrap { flex-wrap: wrap; }
.justify-center { justify-content: center; }
.justify-between { justify-content: space-between; }
.align-center { align-items: center; }
.align-start { align-items: flex-start; }
.align-end { align-items: flex-end; }

/* Text */
.text-center { text-align: center; }
.text-left { text-align: left; }
.text-right { text-align: right; }

/* Responsive visibility */
.mobile-only {
  display: block;
}

@media (min-width: 1024px) {
  .mobile-only {
    display: none;
  }
}

.desktop-only {
  display: none;
}

@media (min-width: 1024px) {
  .desktop-only {
    display: block;
  }
}

/* ========================================
   MODAL
   ======================================== */

/* Overlay */
.modal-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 1000;
  opacity: 0;
  visibility: hidden;
  transition: opacity var(--transition-base), visibility var(--transition-base);
}

.modal-overlay.active {
  opacity: 1;
  visibility: visible;
}

/* Modal */
.modal {
  position: fixed;
  background-color: var(--color-white);
  z-index: 1001;
  opacity: 0;
  visibility: hidden;
  transition: opacity var(--transition-base), 
              visibility var(--transition-base), 
              transform var(--transition-base);
}

.modal.active {
  opacity: 1;
  visibility: visible;
}

/* Request Modal - Mobile (Bottom Sheet) */
.request-modal {
  left: 0;
  bottom: 0;
  width: 100%;
  border-radius: 24px 24px 0 0;
  padding: 24px 16px 20px;
  transform: translateY(100%);
}

.request-modal.active {
  transform: translateY(0);
}

/* Request Modal - Desktop (Centered) */
@media (min-width: 1024px) {
  .request-modal {
    left: 50%;
    top: 50%;
    bottom: auto;
    transform: translate(-50%, -50%) scale(0.95);
    width: 426px;
    border-radius: 24px;
    padding: 25px 20px;
  }
  
  .request-modal.active {
    transform: translate(-50%, -50%) scale(1);
  }
}

/* Close button */
.modal-close {
  position: absolute;
  top: 15px;
  right: 15px;
  width: 24px;
  height: 24px;
  padding: 0;
  border: none;
  background: transparent;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: opacity var(--transition-fast);
}

.modal-close:hover {
  opacity: 0.7;
}

/* Modal Header */
.modal-header {
  display: flex;
  flex-direction: column;
  gap: 8px;
  margin-bottom: 20px;
}

.modal-title {
  font-family: var(--font-primary);
  font-size: 22px;
  font-weight: var(--font-weight-semibold);
  line-height: normal;
  color: var(--color-black-100);
  margin: 0;
}

@media (min-width: 1024px) {
  .modal-title {
    font-size: 28px;
  }
}

.modal-subtitle {
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-medium);
  line-height: normal;
  color: var(--color-black-60);
  margin: 0;
}

@media (min-width: 1024px) {
  .modal-subtitle {
    font-size: 16px;
  }
}

/* Modal Form */
.modal-form {
  display: flex;
  flex-direction: column;
  gap: 12px;
}

.form-group {
  width: 100%;
}

/* Form Input */
.form-input {
  width: 100%;
  padding: 15px 12px;
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-regular);
  line-height: normal;
  color: var(--color-black-100);
  background-color: var(--color-white);
  border: 1px solid var(--color-light-stroke);
  border-radius: 12px;
  outline: none;
  transition: border-color var(--transition-fast);
  box-sizing: border-box;
}

@media (min-width: 1024px) {
  .form-input {
    padding: 16px 15px;
    font-size: 15px;
    border-radius: 15px;
  }
}

.form-input::placeholder {
  color: var(--color-black-60);
}

.form-input:focus {
  border-color: var(--color-black-100);
}

/* Form Select */
.form-select-wrapper {
  position: relative;
  width: 100%;
}

.form-select {
  width: 100%;
  padding: 15px 40px 15px 12px;
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-regular);
  line-height: normal;
  color: var(--color-black-60);
  background-color: var(--color-white);
  border: 1px solid var(--color-light-stroke);
  border-radius: 12px;
  outline: none;
  appearance: none;
  cursor: pointer;
  transition: border-color var(--transition-fast);
  box-sizing: border-box;
}

@media (min-width: 1024px) {
  .form-select {
    padding: 16px 40px 16px 15px;
    font-size: 15px;
    border-radius: 15px;
  }
}

.form-select:focus {
  border-color: var(--color-black-100);
}

.form-select:valid {
  color: var(--color-black-100);
}

.form-select-arrow {
  position: absolute;
  right: 15px;
  top: 50%;
  transform: translateY(-50%);
  pointer-events: none;
}

/* Custom Dropdown */
.custom-dropdown {
  position: relative;
  width: 100%;
}

.custom-dropdown-trigger {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  padding: 15px 12px;
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-regular);
  line-height: normal;
  color: var(--color-black-60);
  background-color: var(--color-white);
  border: 1px solid var(--color-light-stroke);
  border-radius: 12px;
  cursor: pointer;
  transition: border-color var(--transition-fast);
  box-sizing: border-box;
  text-align: left;
}

@media (min-width: 1024px) {
  .custom-dropdown-trigger {
    padding: 16px 15px;
    font-size: 15px;
    border-radius: 15px;
  }
}

.custom-dropdown-trigger:focus,
.custom-dropdown.open .custom-dropdown-trigger {
  border-color: var(--color-black-100);
  outline: none;
}

.custom-dropdown.has-value .custom-dropdown-trigger {
  color: var(--color-black-100);
}

.custom-dropdown-value {
  flex: 1;
}

.custom-dropdown-value .required {
  color: #f85353;
}

.custom-dropdown-arrow {
  flex-shrink: 0;
  transition: transform var(--transition-fast);
}

.custom-dropdown.open .custom-dropdown-arrow {
  transform: rotate(180deg);
}

.custom-dropdown-list {
  position: absolute;
  top: calc(100% + 8px);
  left: 0;
  right: 0;
  background-color: var(--color-white);
  border-radius: 15px;
  box-shadow: 0px 4px 15px 0px rgba(20, 25, 32, 0.2);
  overflow: hidden;
  opacity: 0;
  visibility: hidden;
  transform: translateY(-10px);
  transition: opacity var(--transition-fast), 
              visibility var(--transition-fast), 
              transform var(--transition-fast);
  z-index: 10;
}

.custom-dropdown.open .custom-dropdown-list {
  opacity: 1;
  visibility: visible;
  transform: translateY(0);
}

.custom-dropdown-item {
  padding: 12px;
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-medium);
  line-height: normal;
  color: var(--color-black-100);
  cursor: pointer;
  border-bottom: 1px solid var(--color-light-stroke);
  transition: background-color var(--transition-fast);
}

.custom-dropdown-item:last-child {
  border-bottom: none;
}

.custom-dropdown-item:hover,
.custom-dropdown-item.active {
  background-color: var(--color-light-gray);
}

/* Form Actions */
.form-actions {
  display: flex;
  flex-direction: column;
  gap: 12px;
  margin-top: 8px;
}

.form-submit {
  width: 100%;
  padding: 15px 32px;
}

/* Form Agreement */
.form-agreement {
  font-family: var(--font-primary);
  font-size: 13px;
  font-weight: var(--font-weight-medium);
  line-height: normal;
  color: var(--color-black-60);
  text-align: center;
  margin: 0;
  opacity: 0.6;
}

.form-agreement a {
  color: inherit;
  text-decoration: underline;
}

/* Modal Steps */
.modal-step {
  display: none;
}

.modal-step.active {
  display: flex;
  flex-direction: column;
}

/* Modal Step 1 (Form) */
.modal-step-1.active {
  display: block;
}

/* Modal Success State */
.modal-step-success {
  align-items: center;
  gap: 20px;
}

.modal-success-icon {
  width: 100px;
  height: 100px;
  border-radius: 100px;
  background-color: var(--color-light-gray);
  overflow: hidden;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  flex-shrink: 0;
}

.modal-success-icon img {
  width: 66px;
  height: auto;
  object-fit: contain;
}

.modal-success-content {
  display: flex;
  flex-direction: column;
  gap: 4px;
  align-items: center;
  text-align: center;
  width: 100%;
}

.modal-success-title {
  font-family: var(--font-primary);
  font-size: 22px;
  font-weight: var(--font-weight-semibold);
  line-height: normal;
  color: var(--color-black-100);
  margin: 0;
}

@media (min-width: 1024px) {
  .modal-success-title {
    font-size: 28px;
  }
}

.modal-success-subtitle {
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-medium);
  line-height: normal;
  color: var(--color-black-60);
  margin: 0;
}

@media (min-width: 1024px) {
  .modal-success-subtitle {
    font-size: 16px;
  }
}

.modal-success-buttons {
  display: flex;
  flex-direction: column;
  gap: 12px;
  width: 100%;
}

.modal-success-call {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 5px;
  width: 100%;
  padding: 15px 32px;
}

.modal-success-call svg {
  width: 18px;
  height: 18px;
  flex-shrink: 0;
}

.modal-success-close {
  width: 100%;
  padding: 15px 32px;
}

/* Button Secondary */
.btn-secondary {
  background-color: var(--color-light-gray);
  color: var(--color-black-100);
  border-radius: var(--radius-md);
  font-family: var(--font-primary);
  font-size: 15px;
  font-weight: var(--font-weight-bold);
  line-height: normal;
  border: none;
  cursor: pointer;
  transition: background-color var(--transition-fast);
}

.btn-secondary:hover {
  background-color: var(--color-light-stroke);
}

/* Body scroll lock when modal is open */
body.modal-open {
  overflow: hidden;
}

/* ========================================
   SERVICE REQUEST MODAL (Extended Form)
   ======================================== */

.service-request-modal {
  max-height: 90vh;
  overflow-y: auto;
}

@media (min-width: 1024px) {
  .service-request-modal {
    max-height: 85vh;
  }
}

/* Service Modal Form */
.service-modal-form {
  display: flex;
  flex-direction: column;
  gap: 12px;
}

/* Textarea for comments */
.form-textarea {
  min-height: 81px;
  resize: vertical;
  font-family: var(--font-primary);
}

/* Required asterisk */
.required {
  color: #f85353;
}

/* Custom Dropdown for Service Modal */
.custom-dropdown {
  position: relative;
  width: 100%;
}

.dropdown-trigger {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15px 12px;
  background-color: var(--color-white);
  border: 1px solid var(--color-light-stroke);
  border-radius: 12px;
  cursor: pointer;
  transition: border-color var(--transition-fast);
}

.dropdown-trigger:hover {
  border-color: var(--color-black-60);
}

.custom-dropdown.open .dropdown-trigger {
  border-color: var(--color-black-100);
}

.custom-dropdown.error .dropdown-trigger {
  border-color: #f85353;
}

.dropdown-text {
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-regular);
  line-height: normal;
  color: var(--color-black-60);
}

.dropdown-trigger[data-value]:not([data-value=""]) .dropdown-text {
  color: var(--color-black-100);
}

.dropdown-arrow {
  width: 17px;
  height: 17px;
  flex-shrink: 0;
  transition: transform var(--transition-fast);
}

.custom-dropdown.open .dropdown-arrow {
