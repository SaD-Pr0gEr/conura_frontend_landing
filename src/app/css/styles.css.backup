  flex-direction: column;
  gap: 24px;
  position: relative;
  padding-left: 30px;
}

@media (min-width: 1024px) {
  .how-we-work-steps {
    gap: 16px;
    flex: 0 0 auto;
    width: 450px;
    padding-left: 40px;
