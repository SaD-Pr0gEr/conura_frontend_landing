}

.dropdown-item {
  padding: 12px 16px;
  font-family: var(--font-primary);
  font-size: 14px;
  font-weight: var(--font-weight-regular);
  line-height: normal;
  color: var(--color-black-80);
  cursor: pointer;
  transition: background-color var(--transition-fast), color var(--transition-fast);
